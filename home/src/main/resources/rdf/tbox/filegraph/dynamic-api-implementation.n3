@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dynapiimpl: <java:edu.cornell.mannlib.vitro.webapp.dynapi.components#> .
@prefix dynapi: <https://vivoweb.org/ontology/vitro-dynamic-api#> .


dynapiimpl:Action 
    a owl:Class ;
    rdfs:comment "Processing flow logic" .

dynapi:action rdfs:subClassOf dynapiimpl:Action .

dynapiimpl:OperationalStep
    a owl:Class ;
    rdfs:comment "Processing flow execution unit" .

dynapi:operationalStep rdfs:subClassOf dynapiimpl:OperationalStep .

dynapiimpl:ConditionalStep
    a owl:Class ;
    rdfs:comment "Processing flow conditional" .

dynapi:conditionalStep rdfs:subClassOf dynapiimpl:ConditionalStep .
